<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/lead2loyalty/settings/wcm/templates/product-listing-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="lead2loyalty/components/content/organisms/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="xpbootstrap/components/content/organisms/xp-container">
            <experiencefragment-header
                jcr:created="{Date}2022-12-01T13:45:09.557+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2022-12-01T13:45:22.783+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/experience-fragments/editor/components/experiencefragment"
                fragmentPath="/content/experience-fragments/lead2loyalty/us/en/site/header/master"
                text="Header"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="xpbootstrap/components/content/organisms/xp-container"
                layout="responsiveGrid">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="xpbootstrap/components/content/organisms/xp-container"
                    editable="{Boolean}true"
                    layout="responsiveGrid"/>
            </container>
            <experiencefragment-footer
                jcr:created="{Date}2022-12-01T13:45:09.557+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2022-12-01T13:45:22.783+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/experience-fragments/editor/components/experiencefragment"
                fragmentPath="/content/experience-fragments/lead2loyalty/us/en/site/footer/master"
                text="Footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
