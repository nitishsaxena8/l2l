<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/lead2loyalty/settings/wcm/templates/account-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="lead2loyalty/components/content/organisms/account-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="xpbootstrap/components/content/organisms/xp-container">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/experience-fragments/editor/components/experiencefragment"
                fragmentPath="/content/experience-fragments/lead2loyalty/us/en/site/header/master"
                text="GG Home Products Virtual Reality Pluto 1.0 with M4 Processor Pluto 2.0 with M4 Processor Pluto 3.0 with M4 Processor More Cloud Computing Mercury 1.0 with M2 Processor Mercury 2.0 with M2 Processor Mercury 3.0 with M2 Processor More Imaging Technology Mars 1.0 with M2 Processor Mars 2.0 with M2 Processor Mars 3.0 with M2 Processor More 3D Printing Venus 1.0 with M2 Processor Venus 2.0 with M2 Processor Venus 3.0 with M2 Processor More Robotics Jupiter 1.0 with M3 Processor Jupiter 2.0 with M3 Processor Jupiter 3.0 with M3 Processor More All Products Articles About Us Contact Us Sign In My Account Profile Bookmarks Logout Login Sign in Error Email address Password Sign in Don't have an Account? Register"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="xpbootstrap/components/content/organisms/xp-container"
                layout="responsiveGrid">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="xpbootstrap/components/content/organisms/xp-container"
                    editable="{Boolean}true"
                    layout="responsiveGrid"/>
            </container>
            <experiencefragment-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/experience-fragments/editor/components/experiencefragment"
                fragmentPath="/content/experience-fragments/lead2loyalty/us/en/site/footer/master"
                text="Footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
