<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="cq:Page">
    <jcr:content
		cq:tags="[experience-fragments:variation/web]"
		cq:template="/conf/xpbootstrap/settings/wcm/templates/xf-web-variation"
		cq:xfMasterVariation="{Boolean}true"
		cq:xfVariantType="web"
		jcr:primaryType="cq:PageContent"
		jcr:title="Header"
		sling:resourceType="lead2loyalty/components/content/organisms/xfpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="xpbootstrap/components/content/organisms/root-container"
            layout="responsiveGrid">
            <header
				jcr:primaryType="nt:unstructured"
				sling:resourceType="lead2loyalty/components/content/organisms/header/v1/header"
				profilePageUrl="/content/salesforce-ABM/language-masters/en/my-account/profile"
				bookmarksPageUrl="/content/salesforce-ABM/language-masters/en/my-account/bookmarks"
				logoLink="/content/salesforce-ABM/language-masters/en/home-page"
				logoTitle="Salesforce-ABM"
				ordersPageUrl="/content/salesforce-ABM/language-masters/en/my-account/orders"
				signUpPageUrl="/content/salesforce-ABM/language-masters/en/signup"
				logoutPageUrl="/content/salesforce-ABM/language-masters/en/home-page"
				searchResultPageUrl="/content/salesforce-ABM/language-masters/en/search-result">
                <utilityLinks jcr:primaryType="nt:unstructured">
                    <item0
						jcr:primaryType="nt:unstructured"
						linkUrl="/content/salesforce-ABM/language-masters/en/home-page"
						openInNewWindow="_blank"
						showChildPages="false"
						tr_linkTitle="Home"/>
                    <item1
						jcr:primaryType="nt:unstructured"
						linkUrl="/content/salesforce-ABM/language-masters/en/home-page/products"
						openInNewWindow="_self"
						showChildPages="true"
						tr_linkTitle="Products"/>
                    <item2
						jcr:primaryType="nt:unstructured"
						linkUrl="/content/salesforce-ABM/language-masters/en/article-page"
						openInNewWindow="_self"
						showChildPages="false"
						tr_linkTitle="Articles"/>
                    <item3
						jcr:primaryType="nt:unstructured"
						linkUrl="/content/salesforce-ABM/language-masters/en/about-us"
						openInNewWindow="_self"
						showChildPages="false"
						tr_linkTitle="About Us"/>
                    <item4
						jcr:primaryType="nt:unstructured"
						linkUrl="/content/salesforce-ABM/language-masters/en/contact-us"
						openInNewWindow="_self"
						showChildPages="false"
						tr_linkTitle="Contact Us"/>
                </utilityLinks>
            </header>
        </root>
    </jcr:content>
</jcr:root>
